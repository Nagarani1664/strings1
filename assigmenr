#include <iostream>
#include<string>
using namespace std;
int main() {
   string s;
   getline(cin,s);
    int i=0;
    while(s[i]!='\0'){
        if(i%2!=0) s[i]='#';
        i++;
    }
    cout<<s<<endl;
    return 0;
}




#include <iostream>
#include<string>
using namespace std;
int main() {
   string s;
   getline(cin,s);
   int count=0;
   for(int i=0;s[i]!='\0';i++){
       if(s[i]=='a' || s[i]=='e' || s[i]=='o' || s[i]=='i'  || s[i]=='u' )  continue ;
       else count++;
   }
   cout<<count;
    
}





#include <bits/stdc++.h>
using namespace std;
int main(){
    string s;
    getline(cin,s);
    int n = s.length();
    reverse(s.begin()+n/2,s.end());
    cout<<s;
}






#include<iostream>
using namespace std;
int main() {
    string s;
    cin>>s;
    int x=0;
    int n= s.length();
    for(int i=0;i<n;i++){
        x=x*10;
        x= x+(s[i]-48);
    }
    cout<<x;
    return 0;
}
